{
  "name": "dsc.db",
  "version": "3.0.4-dev.2",
  "description": "A complete and easy to use  MongoDB wrapper.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "docs": "typedoc --theme minimal --out docs src/",
    "pretty": "prettier --config .prettierrc \"./src/**/*.ts\"  \"./lib/**/*.js\" --write",
    "build": "npm run tsc && npm run pretty && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurgenjacobsen/dsc.db.git"
  },
  "keywords": [
    "mongo",
    "wrapper",
    "mongodb",
    "database",
    "mongoose"
  ],
  "author": "jurgenjacobsen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jurgenjacobsen/dsc.db/issues"
  },
  "homepage": "https://github.com/jurgenjacobsen/dsc.db#readme",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "prettier": "^2.3.2",
    "typedoc": "^0.21.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mongoose": "^5.13.7"
  }
}
