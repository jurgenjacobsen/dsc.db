{
  "name": "dsc.db",
  "version": "5.0.0",
  "description": "A MongoDB wrapper to speed up your projects development",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "tsc && node test/index.js",
    "docs": "typedoc --out docs src/ --plugin typedoc-github-theme",
    "pretty": "prettier --config .prettierrc \"./src/**/*.ts\"  \"./lib/**/*.js\" --write",
    "build": "tsc && npm run pretty && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurgenjacobsen/dsc.db.git"
  },
  "keywords": [
    "mongo",
    "mongoose",
    "database"
  ],
  "engines": {
    "node": ">=24.11.0",
    "npm": ">=11.4.1"
  },
  "author": "jurgenjacobsen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jurgenjacobsen/dsc.db/issues"
  },
  "homepage": "https://github.com/jurgenjacobsen/dsc.db#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "mongoose": "^9.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.21",
    "prettier": "^3.7.4",
    "typedoc": "^0.28.15",
    "typedoc-github-theme": "^0.3.1"
  }
}
